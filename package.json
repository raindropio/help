{
  "name": "help",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "local": "env $(cat .env|xargs) docusaurus start --port 3129",
    "local:build": "env $(cat .env|xargs) yarn build",
    "build": "docusaurus build",
    "deploy:prod": "git branch -f release/production master && git push --force origin release/production",
    "postinstall": "sync-dotenv || true",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "2.1.x",
    "@docusaurus/preset-classic": "2.1.x",
    "@mdx-js/react": "1.x",
    "clsx": "^1.1.1",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sync-dotenv": "2.7.x"
  },
  "lint-staged": {
    "*.{md,mdx}": "prettier --write",
    "*.css": "prettier --write",
    "*.js": "prettier --write",
    "*.json": "prettier --write"
  },
  "prettier": {
    "singleQuote": true
  }
}
